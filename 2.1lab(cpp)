#include<iostream>
using namespace std;
namespace first_space
{
void fun()
{
  cout<<"inside first_space"<<endl;
}
}
namespace second_space
{
void fun()
{
  cout<<"inside second_space"<<endl;
}
}
int main()
{
first_space::fun();
second_space::fun();
return 0;
}
OUTPUT:
inside first_space
inside second_space
